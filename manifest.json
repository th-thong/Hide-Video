{
  "name": "Hide Video",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["scripting", "tabs","storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [],
  "background": {
    "service_worker": "background.js"
  },
  "options_ui":{
    "page": "options.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
  
}
